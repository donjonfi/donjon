{
  "name": "donjon-fi-language",
  "displayName": "Donjon FI Language",
  "author": {
    "name": "Jonathan Claes",
    "url": "https://donjon.fi"
  },
  "publisher": "donjon-fi",
  "license": "MIT",
  "homepage": "https://donjon.fi",
  "description": "Syntax highligthing and snippets for Donjon FI lang",
  "version": "2.0.1",
  "icon": "donjon128.png",
  "preview": true,
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/donjonfi/donjon"
  },
  "contributes": {
    "languages": [
      {
        "id": "donjon",
        "aliases": [
          "Donjon",
          "donjon"
        ],
        "extensions": [
          ".djn"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "donjon",
        "scopeName": "source.djn",
        "path": "./syntaxes/donjon.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "donjon",
        "path": "./snippets/donjon.json"
      }
    ]
  }
}