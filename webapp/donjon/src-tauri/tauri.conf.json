{
  "package": {
    "productName": "donjon-creer",
    "version": "0.300.0"
  },
  "build": {
    "distDir": "../dist/donjon-creer",
    "devPath": "http://localhost:4200",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "fi.donjon.creer",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "©2018-2022 Jonathan.claes − donjon.fi",
      "category": "DeveloperTool",
      "shortDescription": "Donjon vous permet de créer vos fictions interactives et d’y jouer.",
      "longDescription": "",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false,
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://donjon.fi/update/donjon-creer.json"
      ],
      "dialog": true
    },
    "allowlist": {
      "all": true
    },
    "windows": [
      {
        "title": "Donjon - Créer",
        "width": 1280,
        "height": 720,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}